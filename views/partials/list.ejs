<% data.map((artItem)=> {

    %>
    <li>
        <a href="/detail/<%= artItem.id.substring(3) %>">
            <figure>
                <img src="<%= artItem.webImage.url %>" alt="image art">
                <figcaption>
                    <%= artItem.title %>
                </figcaption>
            </figure>
        </a>
    </li>

    <% }) %>